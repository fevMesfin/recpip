{
  "name": "recipie",
  "version": "1.0.0",
  "main": "server.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "dev:express": "nodemon server.js",
    "lint": "eslint .",
    "dev": "docker-compose up -d",
    "down": "docker-compose down",
    "all:log": "docker-compose logs -f --tail 100",
    "hasura:init": "hasura init hasura",
    "hasura": "hasura --project hasura --envfile ../.env",
    "hasura:nmc": "hasura --project hasura --envfile ../nmc.env",
    "hasura:prod": "hasura --project hasura --envfile ../.env.prod",
    "console": "yarn hasura console --skip-update-check --api-port 8032 --console-port 8033",
    "hasura:migrate": "yarn hasura migrate apply && yarn hasura metadata apply",
    "hasura:migrate:prod": "yarn hasura:prod migrate apply && yarn hasura metadata apply",
    "hasura:migrate:status": "yarn hasura migrate status",
    "hasura:migrate:status:prod": "yarn hasura:prod migrate status",
    "hasura:seed": "yarn hasura seeds apply",
    "rebuild": "docker-compose build patient_express && docker-compose up -d",
    "start": "node server.js"
  },
  "dependencies": {
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-link-http": "^1.5.17",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.1.3",
    "bcryptjs": "^3.0.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.22.0",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "cross-fetch": "^3.0.5",
    "dotenv": "^8.2.0",
    "exceljs": "^4.3.0",
    "express": "^4.17.1",
    "file-type": "^16.2.0",
    "form-data": "^3.0.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.45",
    "morgan": "^1.10.0",
    "mustache": "^4.0.1",
    "request-promise": "^4.2.6",
    "uuid": "^8.3.0",
    "zemen": "^0.0.7"
  },
  "devDependencies": {
    "hasura-cli": "^1.3.2",
    "nodemon": "^2.0.15"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "UNLICENSED",
  "description": "orbit-core-backend",
  "author": ""
}
